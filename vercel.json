{
  "version": 2,
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/logo.svg",
      "dest": "/logo.svg"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
